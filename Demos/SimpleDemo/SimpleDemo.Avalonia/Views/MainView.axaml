<views:MesAvnView 
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:MinoriEditorShell.Platforms.Avalonia.Views;assembly=MinoriEditorShell.Platforms.Avalonia"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    Background="Aquamarine"
    x:Class="SimpleDemo.Avalonia.Views.MainView">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Vertical">
            <Menu>
                <MenuItem Header="Samples">
                    <MenuItem Header="TipCalc" Command="{Binding TipCalcCommand}" />
                    <MenuItem Header="Settings" Command="{Binding SettingsCommand}" />
                </MenuItem>
            </Menu>
            <!--<ToolBar>
                <Button Content="TipCalc" Command="{Binding TipCalcCommand}" />
            </ToolBar>-->
        </StackPanel>

        <!--<ContentControl DockPanel.Dock="Bottom" cal:View.Model="{Binding StatusBar}" IsTabStop="False" />-->
        <!--<mes:MesDocumentManagerView />-->
    </DockPanel>
</views:MesAvnView>
